	101_10: {
		title: Let Fly the Darts of Lindon
		culture: elven
		twilight: 1
		type: condition
		keyword: tale
		target: unique,elf,companion
		effects: {
			type: activated
			phase: archery
			cost: [
				{
					type: exert
					filter: bearer
					count: 1
				}
			]
			effect: [
				{
					type: memorize
					filter: side(free people),archer
					memory: exarchers
				}
				
				{
					type: addModifier
					modifier: {
						type: addkeyword
						filter: memory(exarchers)
						keyword: damage+1
					}
					until: start(regroup)
				}
				{
					type: addModifier
					modifier: {
						type: removekeyword
						filter: memory(exarchers)
						keyword: archer
					}
					until: start(regroup)
				}
			]
		}
	}


	101_11: {
		title: Library of Rivendell
		unique: true
		culture: elven
		twilight: 3
		type: artifact
		keyword: support area
		effects: [
			{
				type: activated
				phase: fellowship

				cost: {
					type: stackcards
					filter: choose(your,or(tale,artifact),not(self))
					where: self
				}
				effect: {
					type: drawcards
					count: 1
				}
			}
			{
				type: activated
				phase: fellowship
				cost: {
					type: stackcardsfromhand
					filter: choose(your,or(tale,artifact),not(self))
					where: self
				}
				effect: {
					type: drawcards
					count: 1
				}
			}

			{
				type: activated
				phase: regroup
				cost: [
					{
						type: exert
						filter: choose(culture(elven),ally)
					}
				]
				effect: [
					{
						type: memorizestacked
						memory: stackedCards
						on: self
						filter: side(free people)
					}
					{
						type: memorizenumber
						memory: stackedCultures
						amount:{
							type: forEachCulture
							over: 0
							filter: cultureFromMemory(stackedCards)
						}
					}
					{
						type: drawCards
						count: {
							type: fromMemory
							memory: stackedCultures
						}
					}
					{
						type: exert
						filter: choose(minion)
						count: {
							type: fromMemory
							memory: stackedCultures
						}
					}
					{
					    type: repeat
					    times: memory(stackedCultures)
					    effect: {
					        type: heal
					        filter: choose(companion)
					        count: 0-1
					    }
					}
					{
						type: discard
						filter: self
					}
				]
			}
		]
	}


	101_12: {
		title: Three Rings for the Elven-kings
		unique: true
		culture: elven
		twilight: 1
		type: condition
		keyword: [
			support area
			tale
		]
		effects: [
			{
				type: trigger
				optional: true
				text: Would you like to spot 1 [elven] or [gandalf] ring to place a card from hand beneath your draw deck and draw a card?
				trigger: {
					type: moves
				}
				requires: {
					type: canSpot
					filter: or(culture(elven),culture(gandalf)),ring
					count: 1
				}
				effect: [
					{
						type: putCardsFromHandOnBottomOfDeck
						filter: choose(any)
					}
					{
						type: drawCards
						count: 1
					}
				]
			}
			{
				type: trigger
				optional: false
				text: Would you like to spot 3 [elven] or [gandalf] rings to heal a companion?
				trigger: {
					type: moves
				}
				requires: {
					type: canSpot
					filter: or(culture(elven),culture(gandalf)),ring
					count: 3
				}
				effect: {
					type: heal
					filter: choose(companion)
					times: 1
				}
			}
		]
	}

